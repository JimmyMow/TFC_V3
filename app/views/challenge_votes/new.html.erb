<h1>New Challenge Vote</h1>


<%= form_tag(challenge_votes_url, method: 'post') do %>


  <div>
    <%= label_tag :user_id %><br />
    <%= number_field_tag :user_id %>
  </div>

  <div>
    <%= label_tag :challenge_id %><br />
    <%= number_field_tag :challenge_id %>
  </div>

  <div>
    <%= label_tag :vote %><br />
    <%= check_box_tag :vote? %>
  </div>


  <div>
    <%= submit_tag "Create Challenge Vote" %>
  </div>
<% end %>
