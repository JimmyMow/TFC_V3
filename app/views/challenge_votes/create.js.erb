<% publish_to 'http://fanschallengetest.herokuapp.com/games' do %>
  $('#like-challenge-<%= @challenge_vote.challenge_id %>').replaceWith('<%= j(render 'upvote', challenge: @challenge_vote.challenge) %>');
  $('#challenge-score-<%= @challenge_vote.challenge.id %>').replaceWith('<%= j(render 'vote_counter', challenge: @challenge_vote.challenge) %>');
<% end %>
