<h1>New Team</h1>


<%= form_tag(teams_url, method: 'post') do %>


  <div>
    <%= label_tag :coach %><br />
    <%= text_field_tag :coach %>
  </div>

  <div>
    <%= label_tag :name %><br />
    <%= text_field_tag :name %>
  </div>

  <div>
    <%= label_tag :img_url %><br />
    <%= text_field_tag :img_url %>
  </div>


  <div>
    <%= submit_tag "Create Team" %>
  </div>
<% end %>
